{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"target": "ES6",
		"sourceMap": true,
		"inlineSourceMap": false,
		"outDir": "../out",
		"typeRoots": [ "../node_modules/@types/" ],
		"strictNullChecks": false
	},
	"exclude": [
		"node_modules"
	]
}
